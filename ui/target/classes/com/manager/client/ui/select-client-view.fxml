            <TableColumn prefWidth="200" text="Logged clients"/>
        </columns>
      </TableView>
       <Label layoutX="218.0" layoutY="289.0" prefHeight="26.0" prefWidth="164.0" text="Not logged yet? Please log in." />
       <Button layoutX="276.0" layoutY="316.0" mnemonicParsing="false" onAction="#backToLogin" text="Log in" />
       <Label layoutX="201.0" layoutY="341.0" text="You don't have account? Please sign up" />
       <Button layoutX="271.0" layoutY="359.0" mnemonicParsing="false" onAction="#backToSignUp" prefHeight="26.0" prefWidth="58.0" text="Sign Up" />
      <Label layoutX="173.0" layoutY="29.0" text="Select one of logged accounts">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </children>
